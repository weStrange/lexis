    "private": true,
    "proxy": "http://localhost:7000",
    "dependencies": {
        "animate.css": "^3.5.2",
        "apollo-client": "^1.9.3",
        "assets": "file:src/assets",
        "autoprefixer": "7.1.2",
        "babel-core": "6.25.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.1",
        "babel-preset-react-app": "^3.0.2",
        "babel-runtime": "6.26.0",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "chart.js": "^2.7.0",
        "common-components": "file:src/components",
        "core": "file:src/core",
        "css-loader": "0.28.4",
        "dotenv": "4.0.0",
        "eslint": "^4.6.1",
        "eslint-config-react-app": "^2.0.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-flowtype": "2.35.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.1.0",
        "extract-text-webpack-plugin": "3.0.0",
        "file-loader": "0.11.2",
        "fs-extra": "3.0.1",
        "graphql-tag": "^2.4.2",
        "history": "^4.7.2",
        "html-webpack-plugin": "2.29.0",
        "husky": "^0.14.3",
        "immutable": "^3.8.1",
        "jest": "20.0.4",
        "linklocal": "^2.8.1",
        "lint-staged": "^4.1.3",
        "lodash": "^4.17.4",
        "material-ui": "next",
        "material-ui-icons": "^1.0.0-beta.10",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.6",
        "prettier": "^1.6.1",
        "promise": "8.0.1",
        "react": "^15.6.1",
        "react-apollo": "^1.4.15",
        "react-chartjs-2": "^2.6.2",
        "react-dev-utils": "^4.0.1",
        "react-dom": "^15.6.1",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "react-soundplayer": "^0.3.6",
        "react-swipeable-views": "^0.12.8",
        "react-youtube": "^7.4.0",
        "redux": "^3.7.2",
        "redux-saga": "^0.15.6",
        "reselect": "^3.0.1",
        "style-loader": "0.18.2",
        "styled-components": "^2.1.2",
        "sw-precache-webpack-plugin": "0.11.4",
        "tinycolor2": "^1.4.1",
        "url-loader": "0.5.9",
        "webpack": "3.5.1",
        "webpack-dev-server": "2.7.1",
        "webpack-manifest-plugin": "1.2.1",
        "whatwg-fetch": "2.0.3"
    },
    "scripts": {
        "flow": "flow",
        "format": "./node_modules/.bin/prettier-standard \"src/**/*.{js,jsx}\"",
        "lint": "standard \"src/**/*.js\"",
        "precommit": "lint-staged",
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "standard \"src/**/*.js\"&&node scripts/test.js --env=jsdom",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.2.6",
        "@storybook/addon-links": "^3.2.6",
        "@storybook/react": "^3.2.8",
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-decorators": "^6.24.1",
        "babel-preset-flow": "^6.23.0",
        "flow-bin": "^0.54.1",
        "prettier-standard": "^5.1.0",
        "standard": "^10.0.3"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,json,css}": [
            "prettier-standard",
            "git add"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "presets": [
            "react-app",
            "flow"
        ],
        "plugins": [
            "transform-decorators"
        ]
    },
    "standard": {
        "ignore": [
            "registerServiceWorker.js",
            "**/*.test.js"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "prettier": {
        "jsxBracketSameLine": true,
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "semi": false
    }
}
